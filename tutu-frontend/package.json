{
  "name": "tutu-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "cd .. && husky tutu-frontend/.husky",
    "dev": "touch .env.development.local && NODE_EXTRA_CA_CERTS=\"$(mkcert -CAROOT)/rootCA.pem\" node --env-file=.env.development --env-file=.env.development.local dev-server.mjs",
    "test-server": "node --env-file=.env.test dev-server.mjs",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier -u -c *",
    "prettier:fix": "prettier --write -u *",
    "create-dev-certs": "mkdir -p certificates && cd certificates && mkcert localhost && mkcert -install",
    "node-extra-ca-certs": "export NODE_EXTRA_CA_CERTS=\"$(mkcert -CAROOT)/rootCA.pem\"",
    "create-self-signed-dev-certs": "mkdir -p certificates && openssl req -x509 -newkey rsa:4096 -keyout ./certificates/localhost-key.pem -out ./certificates/localhost.pem -sha256 -days 365 -nodes -subj \"/C=XX/ST=StateName/L=CityName/O=CompanyName/OU=CompanySectionName/CN=CommonNameOrHostname\"",
    "playwright-in-docker": "just _playwright-in-docker",
    "start-server-and-test-playwright": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test test-server https://localhost:33123/tutu-frontend playwright-in-docker"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.7",
    "@mui/material": "^6.4.7",
    "@mui/material-nextjs": "^6.4.3",
    "@opetushallitus/oph-design-system": "^1.0.0",
    "@tanstack/react-query": "^5.74.4",
    "@tolgee/format-icu": "^6.2.2",
    "@tolgee/react": "^6.2.4",
    "date-fns": "^4.1.0",
    "next": "15.2.5",
    "nuqs": "^2.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "remeda": "^2.21.3",
    "ts-pattern": "^5.7.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@axe-core/react": "^4.10.1",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.24.0",
    "@playwright/test": "^1.51.0",
    "@tanstack/react-query-devtools": "^5.74.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9",
    "eslint-config-next": "^15.3.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-prettier": "^5.2.3",
    "http-proxy-middleware": "^3.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "3.5.3",
    "start-server-and-test": "^2.0.11",
    "typescript": "^5",
    "vitest": "^3.1.3"
  }
}
