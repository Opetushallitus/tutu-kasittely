{
  "aws": {
    "vpc_cidr": "10.6.128.0/18",
    "availability_zones": 2,
    "domain": "tutu-qa.opintopolku.fi"
  },
  "services": {
    "web_frontend": {
      "env_vars": {
        "NEXT_TELEMETRY_DISABLED": "1",
        "NODE_ENV": "production",
        "VIRKAILIJA_URL": "https://virkailija.testiopintopolku.fi",
        "APP_URL": "https://virkailija.testiopintopolku.fi/tutu-frontend",
        "TUTU_BACKEND": "https://virkailija.testiopintopolku.fi"
      }
    },
    "web_backend": {
      "cpu_limit": "512",
      "memory_limit": "1024",
      "min_count": 1,
      "max_count": 1,
      "allow_ecs_exec": true,
      "env_vars": {
        "LOG_LEVEL": "error",
        "PORT_LISTEN": "8080",
        "HTTP_OPTIONS_TIMEOUT": "5000",
        "HTTP_OPTIONS_RETRY": "2",
        "HTTP_OPTIONS_CLOCK_TOLERANCE": "5",
        "PG_USER": "app",
        "OPINTOPOLKU_DOMAIN": "testiopintopolku.fi"
      }
    }
  },
  "aurora_databases": {
    "web_backend": {
      "version": "16.4",
      "min_size_acu": 0.5,
      "max_size_acu": 2,
      "performance_insights": false
    }
  },
  "cloudfront": {
    "require_test_authentication": false
  }
}
